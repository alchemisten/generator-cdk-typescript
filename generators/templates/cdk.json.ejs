{
  "app": "npx ts-node cdk-stack/bin/<%= projectNameLispCase %>.ts",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    <% if (resources.includes('dynamodb')) { %>"@db:example-table": "example-table",<% } %>
    <% if (resources.includes('s3')) { %>"@s3:example-assets": "example-assets",<% } %>
  }
}
